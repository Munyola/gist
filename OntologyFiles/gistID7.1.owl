	<owl:DatatypeProperty rdf:about="&gist;containedText">
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&gist;hasA">
	</owl:ObjectProperty>
	
	<rdf:Description rdf:about="&gist;identifiedBy">
		<rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
		<rdf:type rdf:resource="&owl;ObjectProperty"/>
		<rdfs:subPropertyOf rdf:resource="&gist;hasA"/>
		<rdfs:comment rdf:datatype="&xs;string">This is like a uri: a thing can have more than one ID, but each of the IDs must refer to a unique thing.</rdfs:comment>
		<rdfs:range rdf:resource="&gist;ID"/>
		<owl:inverseOf rdf:resource="&gist;identifies"/>
	</rdf:Description>
	
	<owl:ObjectProperty rdf:about="&gist;identifies">
	</owl:ObjectProperty>
	
	<rdf:Description rdf:about="&gist;uniqueText">
